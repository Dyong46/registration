{
	"name": "boilerplate",
	"version": "1.0.0",
	"description": "This is a boilerplate for Rely micro services",
	"author": "Rely",
	"private": true,
	"license": "UNLICENSED",
	"engines": {
		"node": "18.x",
		"npm": "9.x"
	},
	"scripts": {
		"licenses": "(license-checker --production && license-checker --production --summary) > LICENSES",
		"prebuild": "rimraf dist",
		"clean": "rimraf dist node_modules package-lock.json ",
		"build": "nest build",
		"gcp-build": "npm run build",
		"start": "npm run start:prod",
		"prestart:dev": "rimraf dist",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node -r dotenv/config dist/src/main",
		"lint": "cross-env DEBUG=eslint:cli-engine eslint -c ../../.eslintrc.js . --fix",
		"lint:nofix": "cross-env DEBUG=eslint:cli-engine eslint -c ../../.eslintrc.js .",
		"migration:create": "bash -c 'typeorm migration:create -f dist/src/ormconfig.js -d src/migrations -n $(git branch --show-current)'",
		"migration:generate": "npm run build && bash -c 'typeorm migration:generate -f dist/src/ormconfig.js -d src/migrations -n $(git branch --show-current)'",
		"migration:show": "typeorm migration:show -f dist/src/ormconfig.js",
		"migration:run": "typeorm migration:run -f dist/src/ormconfig.js",
		"migration:revert": "typeorm migration:revert -f dist/src/ormconfig.js"
	},
	"dependencies": {
		"@google-cloud/debug-agent": "^5.2.9",
		"@google-cloud/storage": "^6.6.0",
		"@nestjs/axios": "^0.1.0",
		"@nestjs/common": "^8.4.7",
		"@nestjs/config": "^2.1.0",
		"@nestjs/core": "^8.4.7",
		"@nestjs/mapped-types": "^1.0.1",
		"@nestjs/mongoose": "^9.2.0",
		"@nestjs/platform-express": "^8.4.7",
		"@nestjs/swagger": "^5.2.1",
		"@nestjs/throttler": "^3.1.0",
		"@nestjs/typeorm": "~8.0.3",
		"@types/multer": "^1.4.7",
		"cache-manager": "^4.0.1",
		"cache-manager-redis-store": "^2.0.0",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.0",
		"cls-hooked": "^4.2.2",
		"compression": "^1.7.4",
		"dotenv": "^16.0.1",
		"elastic-apm-node": "^3.36.0",
		"http-status-codes": "^2.2.0",
		"ioredis": "^5.0.0",
		"json-diff": "^1.0.3",
		"jsonpath": "^1.1.1",
		"mongodb": "^5.1.0",
		"mongoose": "^6.7.0",
		"mysql2": "^2.3.3",
		"redis-semaphore": "^5.3.0",
		"reflect-metadata": "^0.1.13",
		"request-ip": "^3.3.0",
		"rimraf": "^3.0.2",
		"rxjs": "^7.5.5",
		"swagger-themes": "^1.2.26",
		"swagger-ui-express": "^4.6.0",
		"typeorm": "^0.2.45"
	},
	"devDependencies": {
		"@nestjs/cli": "^8.2.6",
		"@nestjs/schematics": "^8.0.11",
		"@types/node": "^18.0.0",
		"eslint": "^8.18.0",
		"husky": "^8.0.1",
		"ts-loader": "^9.3.0",
		"ts-node": "^10.8.1",
		"tsconfig-paths": "^4.0.0",
		"typescript": "<4.8",
		"webpack": "^5.75.0"
	}
}
